{
  "name": "11ty-contentful-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npx eleventy --pathprefix=11ty-contentful-starter",
    "build-dev": "npx eleventy",
    "dev": "cross-env NODE_ENV=development npm-run-all clean styles:dev scripts:dev eleventy:default --parallel eleventy:watch serve --print-label",
    "dev:debug": "DEBUG=* npm run dev",
    "eleventy:default": "eleventy",
    "eleventy:watch": "eleventy --watch",
    "styles:prod": "postcss src/pages/assets/css/*.css --output dist/assets/css/main.css",
    "styles:dev": "postcss src/pages/assets/css/*.css --output dist/assets/css/main.css",
    "scripts:prod": "webpack --mode=production",
    "scripts:dev": "webpack --mode=development",
    "serve": "light-server -c .lightserverrc",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production npm-run-all clean styles:prod scripts:prod eleventy:default",
    "debug": "DEBUG=* npx eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@contentful/rich-text-html-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.2",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "contentful": "^7.14.4",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "light-server": "^2.9.1",
    "luxon": "^1.25.0",
    "marked": "^1.1.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "tailwindcss": "^2.0.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {}
}
