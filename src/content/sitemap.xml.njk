---
permalink: /sitemap.xml
sitemapIgnore: true
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/assets/xsl/sitemap.xsl"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for item in collections.all %}
{%- if item.data.sitemapIgnore !== true %}
  <url>
    <loc>{{ item.url | httpUrl }}</loc>
    {% if item.data.premier.date -%}
    <lastmod>{{ item.data.premier.date | htmlDateString }}</lastmod>
    {%- else -%}
    <lastmod>{{ item.date | htmlDateString }}</lastmod>
    {%- endif %}
    <changefreq>{{ item.data.changefreq | default("monthly") }}</changefreq>
    <priority>{{ item.data.priority | default("0.5") }}</priority>
  </url>
{%- endif %}
{%- endfor %}
</urlset>